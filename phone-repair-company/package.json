{
  "name": "irescue",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "postinstall": "prisma generate"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "reset-admin": "node scripts/reset-admin.mjs",
    "create-admin": "node src/scripts/create-admin.mjs",
    "export": "prisma generate && next build && next export -o out",
    "build-static": "prisma generate && next build",
    "postinstall": "prisma generate",
    "fix-migrations": "node prisma/fix-migrations.mjs",
    "db-setup": "npm run fix-migrations && node prisma/setup-database.mjs",
    "vercel-build": "prisma generate && next build",
    "deploy": "node scripts/deploy.mjs",
    "db-test": "node src/lib/db-test.mjs",
    "apply-migration": "prisma db execute --file ./prisma/migrations/20250402_add_available_hours/migration.sql"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.5.0",
    "@react-three/drei": "^10.0.3",
    "@react-three/fiber": "^9.0.4",
    "@supabase/supabase-js": "^2.48.1",
    "@types/three": "^0.174.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.4.7",
    "heroicons": "^2.2.0",
    "next": "^15.2.2",
    "next-auth": "^4.24.7",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.10.0",
    "psql": "^0.0.1",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "three": "^0.174.0",
    "uuid": "^11.1.0",
    "vercel": "^25.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
